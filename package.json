{
  "name": "rivetzcom-etta-next",
  "version": "1.0.0",
  "description": "SSR UWA for main Rivetz Site, built on Node.js, Express, Next.js, React, and SCSS as part of the 'Etta' dynasty of updates across Rivetz web properties.",
  "homepage": "https://rivetz.com",
  "contributors": [
    "Andrew Defee <adefee@rivetz.com> (@adefee)",
    "Jerry Burke <jerry@rivetz.com> (@jdbgit)",
    "Lexi Sprague <lexi@rivetz.com> (@dyslexion)"
  ],
  "main": "server.js",
  "scripts": {
    "dev": "next build && node server.js",
    "build": "next build",
    "start": "next build && next start -p $PORT",
    "prod": "next build && node server.js",
    "maiden": "npm i && next build && node server.js",
    "test": "jest && standard",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "style": "standard"
  },
  "author": "adefee",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/pro-light-svg-icons": "^5.8.2",
    "@fortawesome/pro-regular-svg-icons": "^5.8.2",
    "@fortawesome/pro-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.0",
    "express": "^4.17.0",
    "html-react-parser": "^0.8.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.1.0",
    "next-i18next": "^0.40.0",
    "raw-loader": "^1.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-markdown": "^4.0.6",
    "react-modal-video": "^1.2.1",
    "uikit": "^3.1.4"
  },
  "standard": {
    "ignore": [
      "/static/js/vendor/",
      "components/mdTheme.js",
      "/static/js/ga.js"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.8.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.2",
    "standard": "*"
  }
}
